+function () {
  var $container     = $('[data-tagging-list]')
  var $formContainer = $('[data-tagging-new]')

  <% if @new_tagging.persisted? %>
    $container
      .append('<%= j render(template: 'taggings/show', format: :html, locals: { tagging: @new_tagging }) %>')
  <% end %>

  $formContainer
    .html('<%= j render('form', format: :html) %>')
    .find('[autofocus]')
    .focus()
}()
